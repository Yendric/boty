{
    "name": "boty",
    "version": "1.0.0",
    "description": "Boty - Yendric discord bot",
    "author": "Yendric Van Roey",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Yendric/boty"
    },
    "scripts": {
        "generate": "npx prisma generate",
        "build": "npm run generate && tsc && tsc-alias",
        "start": "npx prisma migrate deploy && node ./dist/index.js",
        "dev": "npm run generate && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts"
    },
    "devDependencies": {
        "@types/ms": "^0.7.34",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.2",
        "prettier": "3.1.1",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prisma": "^5.22.0",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@discordjs/opus": "^0.9.0",
        "@discordjs/voice": "^0.16.1",
        "@prisma/client": "5.15.1",
        "discord.js": "^14.25.1",
        "dotenv": "^16.6.1",
        "ffmpeg-static": "^5.3.0",
        "libsodium-wrappers": "^0.7.16",
        "ms": "^2.1.3",
        "youtube-search": "^1.1.6",
        "ytdl-core": "^4.11.5"
    }
}
